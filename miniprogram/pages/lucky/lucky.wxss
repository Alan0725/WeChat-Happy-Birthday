/**lucky.wxss**/
.container{
  position: relative;
  background-color: #FDAA6D;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.background-image{
  position: absolute;
}
.container-box{
  position: absolute;
  z-index: 1;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
